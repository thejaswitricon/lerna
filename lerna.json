{
  "packages": [
    "packages/aws/eks/eks-simple/*",
    "packages/aws/ecs/ecs-simple/*"
  ],
  "command": {
    "create": {
      "homepage": "https://github.com/thejaswitricon/lerna.git",
      "license": "MIT"
    },
    "publish": {
      "tempTag": true,
      "ignoreChanges": ["private"]
    },
    "version": {
      "conventionalCommits": true,
      "createRelease": "github",
      "exact": true,
      "message": "chore(release): %s"
    }
  },
  "useWorkspaces": true,
  "npmClient": "npm",
  "version": "independent",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
